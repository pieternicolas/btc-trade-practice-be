{
  "name": "btc-trade-practice-be",
  "version": "1.0.0",
  "description": "Bitcoin trading practice backend side",
  "main": "src/server.js",
  "scripts": {
    "watch": "gulp watch",
    "start": "npm run clean && concurrently --kill-others \"npm run test\" \"npm run watch\"",
    "dev": "npm run start",
    "test": "gulp e2e",
    "build": "npm run clean && NODE_ENV=production webpack",
    "serve": "npm run build && node dist/server.min.js",
    "clean": "rm -rf dist/ && rm .gulp-cache || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pieternicolas/btc-trade-practice-be.git"
  },
