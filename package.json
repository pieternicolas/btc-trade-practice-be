    "bitcoin",
    "btc",
    "node",
    "express",
    "javascript",
    "backend"
  ],
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.1",
    "express": "^4.16.3",
    "express-validator": "^4.2.1",
    "jsonwebtoken": "^8.2.0",
    "knex": "^0.13.0",
    "mongodb": "^3.0.4",
    "mysql": "^2.15.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
